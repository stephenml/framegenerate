##############################################################################
##                                                                          ##
##                             打包所有子目录                                 ##
##                  https://github.com/spmjs/spm-sea                        ##
##                                                                          ##
##############################################################################

SUBDIRS=`ls -d */`

exec:
	for subdir in $(SUBDIRS) ;\
		do\
	 		(cd $$subdir && make)\
	 	done;